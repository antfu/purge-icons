{
  "name": "purge-icons-webpack-plugin",
  "version": "0.5.1",
  "description": "Webpack plugin for PurgeIcons",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "keywords": [
    "webpack-plugin",
    "purge-icons"
  ],
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "homepage": "https://github.com/antfu/purge-icons/tree/master/packages/purge-icons-webpack-plugin#readme",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/purge-icons.git"
  },
  "bugs": "https://github.com/antfu/purge-icons/issues",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts"
  },
  "engines": {
    "node": ">= 12"
  },
  "dependencies": {
    "@purge-icons/core": "^0.5.1",
    "@purge-icons/generated": "^0.5.1",
    "webpack-virtual-modules": "^0.4.1"
  },
  "devDependencies": {
    "@types/webpack": "^4.41.26",
    "tsup": "^3.11.0",
    "typescript": "^4.1.3"
  }
}

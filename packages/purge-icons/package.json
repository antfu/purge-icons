{
  "name": "purge-icons",
  "version": "0.5.1",
  "description": "CLI for PurgeIcons",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "license": "MIT",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/antfu/purge-icons"
  },
  "homepage": "https://github.com/antfu/purge-icons",
  "bugs": "https://github.com/antfu/purge-icons/issues",
  "bin": {
    "purge-icons": "bin/purge-icons.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "purge-icons"
  ],
  "engines": {
    "node": ">= 12"
  },
  "scripts": {
    "prebuild": "esno scripts/prepare.ts",
    "build": "tsup src/index.ts src/cli.ts --format cjs,esm --dts",
    "dev": "esno ./src/cli.ts"
  },
  "dependencies": {
    "@purge-icons/core": "^0.5.1",
    "@types/fs-extra": "^9.0.6",
    "fs-extra": "^9.0.1",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "esno": "^0.4.0",
    "rollup": "^2.36.1",
    "tsup": "^3.11.0",
    "typescript": "^4.1.3"
  }
}
